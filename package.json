{
  "name": "cicd",
  "version": "0.0.3",
  "description": "Serverless Devs CI/CD 组件",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run doc && tsc -w",
    "publish": "npm run build && s cli platform publish",
    "build": "tsc",
    "postbuild": "npm run doc",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.0.*",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "lodash.get": "^4.4.2",
    "tty-table": "^4.1.3",
    "fs-extra": "^10.0.0",
    "path": "^0.12.7",
    "inquirer": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  }
}
